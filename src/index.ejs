<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link type="text/css" href="/assets/main.css" rel="stylesheet" />
    <title>TextMaster Issues</title>
  </head>
  <body>
    <div id="root"></div>
    <%= htmlWebpackPlugin.options.initialState %>
    <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
    <script src="<%= htmlWebpackPlugin.options.filesPrefix %><%= htmlWebpackPlugin.files.chunks[chunk].entry %>"></script>
    <% } %>
  </body>
</html>
